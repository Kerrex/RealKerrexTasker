<link rel="stylesheet" type="text/css" href="fullcalendar.css">
<div id="board-calendar-modal" class="modal">
  <div class="modal-content" id="board-calendar-modal-content">
    <div class="modal-header">
      <span class="close">&times;</span>
      <h2>{{project.name}}</h2>
    </div>
    <div class="row">
      {{#if hasEditPermission}}
        <div class="col-md-2">
          {{not-yet-planned-events project=project}}
        </div>
      {{/if}}
      <div class="col-md-10">
        <div class="modal-body">
        </div>
      </div>
    </div>
  </div>
</div>
